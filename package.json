{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React/Redux and Express apps",
  "main": "server.js",
  "scripts": {
    "build-css": "stylus -c src/static/stylus/ --out src/static/css",
    "watch-css": "yarn run build-css && stylus -c -w src/static/stylus --out src/static/css",
    "client-install": "cd client && npm install",
    "start": "npm-run-all -p watch-css start-js node server.js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Brad Traversy & Gregory Assasie",
  "license": "MIT",
  "devDependencies": {},
  "dependencies": {
    "bootstrap": "^4.1.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "global": "^4.3.2",
    "nodemon": "^1.14.7",
    "npm-run-all": "^4.1.3",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.3.0",
    "stylus": "^0.54.5"
  }
}
